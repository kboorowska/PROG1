#include <stdio.h>
 
#define TEMP_WRZENIA 100
#define TEMP_ZAMARZANIA 0
 
int main()
{
    int temp;
    printf("Podaj temperaturne w stopniach Celcjusza :");
    scanf("%d", &temp);
 
 
    if(temp > TEMP_WRZENIA)
    {
        printf("Stan Gazowy");
    }
    else if(temp < TEMP_ZAMARZANIA)
    {
        printf("Stan Stały");
    }
    else
    {
        printf("Stan Ciekły");
    }
 
    return 0;
}
